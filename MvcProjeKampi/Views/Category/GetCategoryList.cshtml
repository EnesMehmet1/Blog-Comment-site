@using EntityLayer.Concrete
@model List<Category>
@{
    Layout = null;
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<br />
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Description</th>
        <th>Status</th>
    </tr>   
    @foreach(var x in Model)
            {
                <tr>
                    <td>@x.CategoryId</td>
                    <td>@x.CategoryName</td>
                    <td>@x.CategoryDescription</td>
                    <td>@x.CategoryStatus</td>
                </tr>
            }
</table>
<a href="/Category/AddCategory" class="btn btn-danger">Yeni Kategori</a>